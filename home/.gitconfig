[include]
        path = ~/.gitconfig.local
[alias]
	co = checkout
	ci = commit -a
	st = status
	unstage = reset HEAD --
	last = log -1 HEAD
        hist = log --pretty=format:\"%h %ad | %s %Cblue%d%Creset [%an]\" --graph --date=short
[core]
	editor = vim -c \":normal gg\"
        whitespace = trailing-space,space-before-tab
	excludesfile = /Users/mortigenus/.gitignore_global
[color]
	ui = auto

